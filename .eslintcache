xableWarningCount":0},"/home/seemam/Web Development/React-Practice/shopping-list-app/src/index.js",[],"/home/seemam/Web Development/React-Practice/shopping-list-app/src/App.js",[],["59","60"],"/home/seemam/Web Development/React-Practice/shopping-list-app/src/components/navbar.component.js",[],"/home/seemam/Web Development/React-Practice/shopping-list-app/src/components/new-list.component.js",[],"/home/seemam/Web Development/React-Practice/shopping-list-app/src/components/manage-catalog.component.js",[],"/home/seemam/Web Development/React-Practice/shopping-list-app/src/components/list-table.component.js",["61"],"import React, { useContext, useEffect } from 'react'\n\nimport {ListsContext} from \"../contexts/lists.context\"\nimport ListRow from \"./listrow.component\"\n\nfunction ListTable(props){\n    const {lists, getLists, error, loading} = useContext(ListsContext)\n\n    useEffect(() => {\n        getLists()\n    },[])\n    \n    return (\n        <>\n            {error}\n            <thead>\n                <th>Description</th>\n                <th>Shopping Date</th>\n                <th>Actions</th>\n            </thead>\n            {loading ? <h3>Loading</h3> : lists.map(list => <ListRow list={list} key={list._id}/>)}\n        </>\n    )\n}\nexport default ListTable;\n","/home/seemam/Web Development/React-Practice/shopping-list-app/src/contexts/lists.context.js",["62"],"//ListContext provides the lists for a user and \n//functions that make backend calls. \n//These functions also call dispatch function of reducer to update front end state\n\nimport {createContext, useReducer} from \"react\"\nimport axios from \"axios\"\n\nimport reducer from \"../reducers/lists.reducer\"\n\nexport const ListsContext = createContext()\n\nconst defaultList = [\n    {description: \"Sample list\", shoppingdate: new Date(), createddate: new Date() }\n]\nconst initialState = {\n    lists: [],\n    error: null,\n    loading: true\n}\nconst LIST_API_URL = \"http://localhost:5000/lists/\"\n\nexport function ListsProvider(props){\n    const [state, dispatch] = useReducer(reducer, initialState)\n    \n    function getLists(){\n        axios.get(LIST_API_URL+\"seema\")\n            .then(res => {\n                dispatch({\n                    type: \"GET_LISTS\",\n                    payload: res.data.data\n                    // payload: defaultList\n                })\n            })\n            .catch(err => {\n                dispatch({\n                    type: \"LISTS_ERROR\",\n                    payload: err\n                })\n            })\n    }\n    function deleteList(listId){\n        axios.delete(LIST_API_URL+`id/${listId}`)\n            .then(res => {\n                dispatch({\n                    type: \"REMOVE\",\n                    payload: listId\n                })\n            })\n            .catch(err => {\n                dispatch({\n                    type: \"LISTS_ERROR\",\n                    payload: err\n                })\n            })\n    }\n    function addList(list){\n        axios.post(LIST_API_URL+\"seema/add\", list)\n            .then(res => {\n                dispatch({\n                    type: \"ADD\",\n                    payload: list\n                })\n            })\n            .catch(err => {\n                dispatch({\n                    type: \"LISTS_ERROR\",\n                    payload: err\n                })\n            })\n    }\n    return(\n        <ListsContext.Provider value={{\n            lists: state.lists,\n            error: state.error,\n            loading: state.loading,\n            getLists,\n            deleteList,\n            addList\n        }}>\n            {props.children}\n        </ListsContext.Provider>\n    )\n}","/home/seemam/Web Development/React-Practice/shopping-list-app/src/components/listrow.component.js",["63"],"import React,{useContext} from \"react\"\nimport { ListsContext } from \"../contexts/lists.context\"\n\nfunction ListRow(props){\n    const {_id, description, shoppingdate} = props.list\n    const {deleteList} = useContext(ListsContext)\n\n    return(\n        <tr style={{padding:\"10px\"}}>\n            <td style={{padding:\"10px\"}}>{description}</td>\n            <td style={{padding:\"10px\"}}>{shoppingdate.substring(0,10)}</td>\n            <td><button onClick={() => deleteList(_id)}>Delete</button></td>\n        </tr>\n    )\n}\nexport default ListRow","/home/seemam/Web Development/React-Practice/shopping-list-app/src/reducers/lists.reducer.js",[],"/home/seemam/Web Development/React-Practice/shopping-list-app/src/components/catalog.component.js",["64"],"import {useContext, useEffect} from \"react\"\nimport { CatalogContext } from \"../contexts/catalog.context\"\n\nfunction Category({categoryname, items}){\n    return(\n        <div>\n            <h5>{categoryname}</h5>\n            {items.map(item => <Item name={item}/>)}\n        </div>\n    )\n}\nfunction Item(props){\n    const itemname = props.name\n    return(\n        <>\n            <input type=\"checkbox\" value={itemname}/>\n            <label htmlFor=\"{itemname}\">{itemname}</label>\n        </>\n    )\n}\nfunction Catalog(props){\n    const {catalog, getCatalog} = useContext(CatalogContext)\n\n    useEffect(() => {\n        getCatalog()\n    },[])\n    return(\n        <div>\n            <h3>Categories:</h3>\n            {catalog.map(category => <Category {...category} />)}\n        </div>\n    )\n}\nexport default Catalog","/home/seemam/Web Development/React-Practice/shopping-list-app/src/contexts/catalog.context.js",[],"/home/seemam/Web Development/React-Practice/shopping-list-app/src/reducers/category.reducer.js",[],"/home/seemam/Web Development/React-Practice/shopping-list-app/src/reportWebVitals.js",[],{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","replacedBy":"68"},{"ruleId":"69","severity":1,"message":"70","line":11,"column":7,"nodeType":"71","endLine":11,"endColumn":9,"suggestions":"72"},{"ruleId":"73","severity":1,"messag